<svg xmlns="http://www.w3.org/2000/svg" width="900" height="400" viewBox="0 0 900 400">
  <!-- Ticket Background -->
  <rect width="900" height="400" fill="white" stroke="#ddd" stroke-width="4" rx="10"/>

  <!-- Left section (main ticket) -->
  <rect x="0" y="0" width="700" height="400" fill="linear-gradient(135deg, #667eea 0%, #764ba2 100%)" rx="10"/>

  <!-- Event details -->
  <text x="50" y="80" font-family="Arial" font-size="32" font-weight="bold" fill="white">SPECIAL EVENT</text>
  <text x="50" y="120" font-family="Arial" font-size="20" fill="white" opacity="0.9">Exclusive Group Activity</text>

  <!-- Date and time -->
  <rect x="50" y="160" width="280" height="80" fill="white" opacity="0.2" rx="8"/>
  <text x="70" y="190" font-family="Arial" font-size="14" fill="white" font-weight="bold">DATE</text>
  <text x="70" y="220" font-family="Arial" font-size="24" fill="white">March 17, 2026</text>

  <rect x="370" y="160" width="280" height="80" fill="white" opacity="0.2" rx="8"/>
  <text x="390" y="190" font-family="Arial" font-size="14" fill="white" font-weight="bold">TIME</text>
  <text x="390" y="220" font-family="Arial" font-size="24" fill="white">7:00 PM</text>

  <!-- Venue -->
  <text x="50" y="280" font-family="Arial" font-size="14" fill="white" opacity="0.8">VENUE</text>
  <text x="50" y="310" font-family="Arial" font-size="20" fill="white">Premium Event Center</text>

  <!-- Seat info -->
  <text x="50" y="360" font-family="Arial" font-size="14" fill="white" opacity="0.8">SECTION: A | ROW: 5 | SEATS: 12-15</text>

  <!-- Right section (stub) -->
  <line x1="700" y1="50" x2="700" y2="350" stroke="white" stroke-width="2" stroke-dasharray="10,5"/>

  <rect x="700" y="0" width="200" height="400" fill="#f8f9fa" rx="10"/>

  <text x="800" y="100" font-family="Arial" font-size="16" font-weight="bold" fill="#333" text-anchor="middle">TICKET</text>
  <text x="800" y="140" font-family="Arial" font-size="14" fill="#666" text-anchor="middle">MAR 17</text>
  <text x="800" y="170" font-family="Arial" font-size="14" fill="#666" text-anchor="middle">2026</text>

  <!-- Barcode representation -->
  <g transform="translate(730, 220)">
    <rect x="0" y="0" width="4" height="80" fill="#333"/>
    <rect x="8" y="0" width="2" height="80" fill="#333"/>
    <rect x="14" y="0" width="6" height="80" fill="#333"/>
    <rect x="24" y="0" width="2" height="80" fill="#333"/>
    <rect x="30" y="0" width="4" height="80" fill="#333"/>
    <rect x="38" y="0" width="2" height="80" fill="#333"/>
    <rect x="44" y="0" width="6" height="80" fill="#333"/>
    <rect x="54" y="0" width="4" height="80" fill="#333"/>
    <rect x="62" y="0" width="2" height="80" fill="#333"/>
    <rect x="68" y="0" width="4" height="80" fill="#333"/>
    <rect x="76" y="0" width="6" height="80" fill="#333"/>
    <rect x="86" y="0" width="2" height="80" fill="#333"/>
    <rect x="92" y="0" width="4" height="80" fill="#333"/>
    <rect x="100" y="0" width="2" height="80" fill="#333"/>
    <rect x="106" y="0" width="6" height="80" fill="#333"/>
    <rect x="116" y="0" width="4" height="80" fill="#333"/>
    <rect x="124" y="0" width="2" height="80" fill="#333"/>
    <rect x="130" y="0" width="6" height="80" fill="#333"/>
  </g>

  <text x="800" y="330" font-family="Arial" font-size="10" fill="#999" text-anchor="middle">ADMIT ONE</text>
</svg>